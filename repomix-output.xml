          name="NewUser"
          component={NewUser}
          options={{ headerShown: true }}
        />
        <Stack.Screen name="Home" component={Home} />
        <Stack.Screen name="Chat" component={Chat} />
      </Stack.Navigator>
    </NavigationContainer>
  );
}
